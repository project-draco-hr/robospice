{
  this.contextWeakReference=new WeakReference<Context>(context);
  if (runner != null) {
    throw new IllegalStateException("Already started.");
  }
 else {
    runner=new Thread(this);
    runner.setPriority(Thread.MIN_PRIORITY);
    isStopped=false;
    runner.start();
    Ln.d("SpiceManager started.");
  }
}
