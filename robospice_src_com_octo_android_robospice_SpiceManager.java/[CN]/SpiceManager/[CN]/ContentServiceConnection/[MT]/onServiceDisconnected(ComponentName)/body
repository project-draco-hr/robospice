{
  try {
    Ln.d("Unbound from service start");
    lockAcquireService.lock();
    Ln.d("Unbound from service : " + spiceService.getClass().getSimpleName());
    spiceService=null;
    isUnbinding=false;
    conditionServiceUnbound.signalAll();
  }
  finally {
    lockAcquireService.unlock();
  }
}
