{
  try {
    if (isAsyncSaveEnabled) {
      new Thread(){
        @Override public void run(){
          try {
            saveData(data,cacheKey);
          }
 catch (          IOException e) {
            Ln.e(e,"An error occured on saving request " + cacheKey + " data asynchronously");
          }
catch (          CacheSavingException e) {
            Ln.e(e,"An error occured on saving request " + cacheKey + " data asynchronously");
          }
 finally {
            lock.lock();
            try {
              condition.signal();
            }
  finally {
              lock.unlock();
            }
          }
        }
      }
.start();
    }
 else {
      saveData(data,cacheKey);
    }
  }
 catch (  CacheSavingException e) {
    throw e;
  }
catch (  Exception e) {
    throw new CacheSavingException(e);
  }
  return data;
}
